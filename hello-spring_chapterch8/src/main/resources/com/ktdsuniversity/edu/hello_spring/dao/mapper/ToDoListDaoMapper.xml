<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ktdsuniversity.edu.hello_spring.dao.ToDoListDao">

	<insert id="insertNewToDoList" parameterType="com.ktdsuniversity.edu.hello_spring.vo.ToDoListWriteVO">
		INSERT INTO TODOLIST
		(ID
		, SBJCT
		, DEADLN)
		VALUES
		(SEQ_TODOLIST_PK.NEXTVAL
		, #{subject}
		, #{deadLine})
	</insert>
</mapper>